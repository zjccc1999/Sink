    "baseline-browser-mapping": "^2.9.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^9.39.2",
    "eslint-plugin-better-tailwindcss": "^3.8.0",
    "eslint-plugin-format": "^1.2.0",
    "lint-staged": "^16.2.7",
    "nuxt": "^4.2.2",
    "shadcn-nuxt": "^2.4.3",
    "simple-git-hooks": "^2.13.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.2.2",
    "wrangler": "^4.58.0"
  },
  "pnpm": {

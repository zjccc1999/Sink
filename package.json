    "reka-ui": "^2.7.0",
    "tw-animate-css": "^1.4.0",
    "ua-parser-js": "^2.0.7",
    "vaul-vue": "^0.4.1",
    "vee-validate": "^4.15.1",
    "virtua": "^0.48.2",
    "vue-sonner": "^2.0.9",
